{
  "name": "tradeduck-backend",
  "version": "0.0.0",
  "description": "TypeScript + Prisma + MySQL + Express API Server",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "npm run build:backend && cross-env NODE_ENV=production node dist/server.js",
    "setup": "docker-compose up -d && yarn prisma:migrate && yarn prisma:seed",
    "dev": "concurrently --kill-others \"yarn run dev:backend\" \"yarn run dev:frontend\"",
    "dev:backend": "yarn prisma:generate && nodemon",
    "dev:frontend": "vite frontend",
    "build": "yarn run build:backend && yarn run build:frontend",
    "build:backend": "swc src -d dist --source-maps --copy-files",
    "build:backend:tsc": "tsc && tsc-alias",
    "build:frontend": "vite build frontend",
    "test:backend": "jest --forceExit --detectOpenHandles",
    "lint:backend": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:frontend": "eslint --ignore-path .gitignore --ext .ts,.vue -c frontend/.eslintrc frontend/",
    "lint:backend:fix": "npm run lint:backend -- --fix",
    "lint:frontend:fix": "npm run lint:frontend -- --fix",
    "prisma:init": "prisma init",
    "prisma:seed": "prisma db seed",
    "prisma:migrate": "prisma migrate dev --preview-feature",
    "prisma:dbReset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:fmt": "prisma format"
  },
  "prisma": {
    "seed": "ts-node src/prisma/seed.ts",
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@auth0/auth0-vue": "^2.2.1",
    "@popperjs/core": "^2.11.8",
    "@prisma/client": "^4.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.3",
    "bcrypt": "^5.0.1",
    "bootstrap": "^5.3.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "concurrently": "^8.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "countries-list": "^2.6.1",
    "country-flag-icons": "^1.5.7",
    "dotenv": "^16.0.1",
    "envalid": "^7.3.1",
    "express": "^4.18.1",
    "express-rate-limit": "^7.1.3",
    "flag-icons": "^6.11.1",
    "helmet": "^5.1.1",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "material-dashboard": "^3.0.9",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.5",
    "pinia": "^2.1.6",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.64.0",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.5.0",
    "typedi": "^0.10.0",
    "url": "^0.11.1",
    "vite-plugin-static-copy": "^0.17.0",
    "vue": "^3.3.4",
    "vue-eslint-parser": "^9.3.1",
    "vue-router": "4",
    "vue-search-select": "^3.2.0",
    "vue-toast-notification": "^3.0",
    "vuetify": "^3.3.13",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.11",
